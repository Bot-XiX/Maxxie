  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "baseUrl": "./",
    "paths": {
      "@root/*": ["./*"],
      "@interactions/*": ["./src/interactions/*"],
      "@schemas/*": ["./src/schemas/*"],
      "@src/*": ["./src/*"],
      "@utils/*": ["./src/utils/*"]
    }
  },
  "exclude": ["node_modules"]
}
